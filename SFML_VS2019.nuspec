<?xml version="1.0"?>
<package >
  <metadata>
    <id>SFML_VS2019</id>
    <version>0.9.5</version>
    <title>SFML_VS2019</title>
    <authors>Jeffrey Kesselman</authors>
    <owners>Jeffrey Kesselman</owners>
    <description>
      SFML native package for Visual Studio 2019
      This package currently only supports 32 bit builds
    </description>
    <releaseNotes></releaseNotes>
    <summary>
      SFML native VS2019
    </summary>
    <language>en-US</language>
    <projectUrl>https://sfml-dev.org</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://www.sfml-dev.org/license.php</licenseUrl>
    <copyright></copyright>
    <dependencies>
    </dependencies>
    <references></references>
    <tags>native</tags> <!-- This is important ! -->
  </metadata>
  <files>
      <!-- Note that now, binaries are placed conventionally under a directory named build -->
      <file src="Win32\bin\**" target="build\native\Win32\bin" />
      <file src="Win32\lib\**" target="build\native\Win32\lib" />
      <file src="Win32\include\**" target="build\native\Win32\include" />
      <file src="Win32\doc\**" target="Win32\doc" />
      <file src="x64\bin\**" target="build\native\x64\bin" />
      <file src="x64\lib\**" target="build\native\x64\lib" />
      <file src="x64\include\**" target="build\native\x64\include" />
      <file src="x64\doc\**" target="x64\doc" />
      <file src="SFML_VS2019.props" target="build" />
      <file src="SFML_VS2019.targets" target="build" />
  </files>
</package>